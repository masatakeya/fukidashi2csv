{
  "manifest_version": 3,
  "name": "ふきだしくんエクスポート",
  "version": "1.0",
  "description": "投稿、名前、いいねの数、付箋の色(RGB値)をCSV形式でクリップボードにコピーします",
  "permissions": ["activeTab", "scripting", "clipboardWrite"],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
